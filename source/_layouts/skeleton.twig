<!DOCTYPE html>
<html lang="en">
<head>
<title>{% block title %}{{ page.title }} &mdash; {{ site.title }}{% endblock %}</title>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
{% block metadata %}{% endblock %}
<link rel="stylesheet" href="{{ site.url }}/css/font-awesome.min.css" />
<link rel="stylesheet" href="{{ site.url }}/css/bootstrap.min.css" />
<link rel="stylesheet" href="{{ site.url }}/css/common.css" />
{% block stylesheets %}{% endblock %}
</head>
<body>
{% block banner %}{% include 'skeleton.banner' %}{% endblock %}
{% block toolbar %}{% endblock %}
{% block main %}{% endblock %}
<script src="{{ site.url }}/js/jquery.slim.min.js"></script>
<script src="{{ site.url }}/js/popper.min.js"></script>
<script src="{{ site.url }}/js/bootstrap.min.js"></script>
<script src="{{ site.url }}/js/react.production.min.js"></script>
<script src="{{ site.url }}/js/react-dom.production.min.js"></script>
<script src="{{ site.url }}/js/redux.min.js"></script>
<script src="{{ site.url }}/js/react-redux.min.js"></script>
{% block script %}{% endblock %}
</body>
</html>
